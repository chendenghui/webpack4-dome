    "less-loader": "^4.1.0",
    "node-sass": "^4.9.0",
    "npm-check": "^5.6.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.3",
    "pre-commit": "^1.2.2",
    "precss": "^3.1.2",
    "react-dev-utils": "^5.0.0",
    "react-hot-loader": "^4.0.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.20.3",
    "styled-components": "^3.3.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
