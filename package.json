{
  "name": "webpack4Demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "entry": "node config/entry/entryBuild.js",
    "devBuildHtml": "node config/webpack/webpack.devBuildHtml.conf.js",
    "devNew": "npm run entry && npm run devBuildHtml",
    "dev": "webpack-dev-server --devtool eval --progress --colors --profile --config config/webpack/webpack.dev.conf.js",
    "build": "export BABEL_ENV=production && webpack --progress --colors --config config/webpack/webpack.prod.conf.js",
    "update": "npm-check -u",
    "analyz": "NODE_ENV=production npm_config_report=true npm run build",
    "clean": "rimraf dist && mkdir dist",
    "eslint": "echo \"代码检测中...\" && eslint ./src *.js,.js --cache",
    "lint": "eslint --ext .js src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "eslint"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.6.1",
    "babel-polyfill": "^6.26.0",
    "class-names": "^1.0.0",
    "immutable": "^3.8.2",
    "less": "^3.0.4",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-masonry-component": "^6.2.1",
    "react-router": "^4.2.0",
    "react-transition-group": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "cache-loader": "^1.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file": "^0.2.2",
    "file-loader": "^1.1.11",
